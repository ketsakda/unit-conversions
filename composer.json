{
    "name": "kravanh/unit-conversions",
    "description": "This is my package UnitConversions",
    "keywords": [
        "kravanh",
        "unit conversions"
    ],
    "homepage": "https://github.com/ketsakda/unit-conversions",
    "license": "MIT",
    "authors": [
        {
            "name": "ket sakda",
            "email": "ketsakda@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.10",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Kravanh\\UnitConversions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kravanh\\UnitConversions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
